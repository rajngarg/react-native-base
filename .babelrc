{
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx",
          ".android.js",
          ".android.tsx",
          ".ios.js",
          ".ios.tsx"
        ],
        "alias": {
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@elements": "./src/elements",
          "@screens": "./src/screens",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}